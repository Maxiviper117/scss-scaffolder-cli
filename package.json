{
    "name": "scss-scaffolder-cli",
    "version": "0.2.0",
    "description": "CLI tool for scaffolding scss folder and files",
    "type": "module",
    "main": "dist/index.js",
    "bin": {
        "scss-cli": "dist/index.js"
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Maxiviper117/scss-scaffolder-cli.git"
    },
    "keywords": [
        "cli",
        "scss",
        "scaffold"
    ],
    "scripts": {
        "build": "bun build ./src/index.ts --outfile ./dist/index.js --target node",
        "compile": "bun build ./src/index.ts --compile --outfile ./compiled/scss-scaffolder-cli"
    },
    "author": "Maxiviper117",
    "license": "GPL-3.0-or-later",
    "dependencies": {
        "chalk": "^5.4.1",
        "commander": "^13.0.0",
        "inquirer": "^12.3.0",
        "typescript": "^5.7.2"
    },
    "devDependencies": {
        "@types/node": "^22.10.5",
        "eslint": "^9.17.0",
        "prettier": "^3.4.2",
        "ts-node": "^10.9.2"
    }
}
